  "flow": {
    "triggers": [
      {
        "id": "button1_scene",
        "title": {
          "en": "A scene has been activated",
          "nl": "Een scene is geactiveerd"
        },
        "hint": {
          "en": "This card will only get triggered if the scene matches the state of the dropdown fields.",
          "nl": "Deze kaart wordt alleen geactiveerd wanneer de scene overeenkomt met de toestand van de gekozen opties."
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=sensor_switch.aq2|sensor_86sw1lu"
          },
          {
            "name": "scene",
            "type": "dropdown",
            "values": [
              {
                "id": "Key Pressed 1 time",
                "label": {
                  "en": "Pressed 1x",
                  "nl": "1x ingedrukt"
                }
              },
              {
                "id": "Key Pressed 2 times",
                "label": {
                  "en": "Pressed 2x",
                  "nl": "2x ingedrukt"
                }
              },
              {
                "id": "Key Pressed 3 times",
                "label": {
                  "en": "Pressed 3x",
                  "nl": "3x ingedrukt"
                }
              },
              {
                "id": "Key Pressed 4 times",
                "label": {
                  "en": "Pressed 4x",
                  "nl": "4x ingedrukt"
                }
              }
            ]
          }
        ]
      },
      {
        "id": "button1_scene_held",
        "title": {
          "en": "A scene has been activated",
          "nl": "Een scene is geactiveerd"
        },
        "hint": {
          "en": "This card will only get triggered if the scene matches the state of the dropdown fields.",
          "nl": "Deze kaart wordt alleen geactiveerd wanneer de scene overeenkomt met de toestand van de gekozen opties."
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=sensor_switch"
          },
          {
            "name": "scene",
            "type": "dropdown",
            "values": [
              {
                "id": "Key Pressed 1 time",
                "label": {
                  "en": "Pressed 1x",
                  "nl": "1x ingedrukt"
                }
              },
              {
                "id": "Key Pressed 2 times",
                "label": {
                  "en": "Pressed 2x",
                  "nl": "2x ingedrukt"
                }
              },
              {
                "id": "Key Pressed 3 times",
                "label": {
                  "en": "Pressed 3x",
                  "nl": "3x ingedrukt"
                }
              },
              {
                "id": "Key Pressed 4 times",
                "label": {
                  "en": "Pressed 4x",
                  "nl": "4x ingedrukt"
                }
              },
              {
                "id": "Key Held Down",
                "label": {
                  "en": "Held Down",
                  "nl": "Lang ingedrukt"
                }
              },
              {
                "id": "Key Released",
                "label": {
                  "en": "Released",
                  "nl": "Losgelaten"
                }
              }
            ]
          }
        ]
      },
      {
        "id": "button1_button",
        "title": {
          "en": "A button has been pressed",
          "nl": "Een knop is ingedrukt"
        },
        "hint": {
          "en": "This card will trigger on any button pressed and provides the scene tokens.",
          "nl": "Deze kaart wordt geactiveerd bij iedere knop die ingedrukt wordt en biedt de scene tokens aan."
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=sensor_switch|sensor_switch.aq2|sensor_86sw1lu"
          }
        ],
        "tokens": [
          {
            "name": "scene",
            "type": "string",
            "title": {
              "en": "scene",
              "nl": "scene"
            },
            "example": "Button pressed (test)"
          }
        ]
      },
      {
        "id": "button2_scene",
        "title": {
          "en": "A scene has been activated",
          "nl": "Een scene is geactiveerd"
        },
        "hint": {
          "en": "This card will only get triggered if the scene matches the state of the dropdown fields.",
          "nl": "Deze kaart wordt alleen geactiveerd wanneer de scene overeenkomt met de toestand van de gekozen opties."
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=sensor_86sw2Un"
          },
          {
            "name": "button",
            "type": "dropdown",
            "values": [
              {
                "id": "Left button",
                "label": {
                  "en": "Left button",
                  "nl": "Linker knop"
                }
              },
              {
                "id": "Right button",
                "label": {
                  "en": "Right button",
                  "nl": "Rechter knop"
                }
              }
            ]
          },
          {
            "name": "scene",
            "type": "dropdown",
            "values": [
              {
                "id": "Key Pressed 1 time",
                "label": {
                  "en": "Pressed 1x",
                  "nl": "1x ingedrukt"
                }
              }
            ]
          }
        ]
      },
      {
        "id": "button2_button",
        "title": {
          "en": "A button has been pressed",
          "nl": "Een knop is ingedrukt"
        },
        "hint": {
          "en": "This card will trigger on any button pressed and provides the scene tokens.",
          "nl": "Deze kaart wordt geactiveerd bij iedere knop die ingedrukt wordt en biedt de scene tokens aan."
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=sensor_86sw2Un"
          }
        ],
        "tokens": [
          {
            "name": "button",
            "type": "string",
            "title": {
              "en": "button",
              "nl": "knop"
            },
            "example": "0"
          },
          {
            "name": "scene",
            "type": "string",
            "title": {
              "en": "scene",
              "nl": "scene"
            },
            "example": "Button pressed (test)"
          }
        ]
      }
    ]
  },
